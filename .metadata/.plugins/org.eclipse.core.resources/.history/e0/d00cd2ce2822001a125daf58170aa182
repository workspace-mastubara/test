package com.internousdev.multi.action;

import java.util.ArrayList;
import java.util.List;
import java.util.Map;
import java.util.TreeMap;

import org.apache.struts2.interceptor.SessionAware;

import com.internousdev.multi.util.Person;
import com.opensymphony.xwork2.ActionSupport;

public class multipleDisplayAction extends ActionSupport implements SessionAware{
	public static void main(String[] args) {
		Map<String, Object> session = new TreeMap<String, Object>();
		Person sato = new Person("sato", 28);
		Person monzen = new Person("monzen", 22);
		Person suzuki = new Person("suzuki", 31);

		List<Person> list = new ArrayList<Person>();
			list.add(sato);
			list.add(monzen);
			list.add(suzuki);

			session.put("list", list);
		}


	public String execute(){
		return SUCCESS;
	}

	public Map<String, Object> getSession(){
		return session;
	}
	public void setSession(Map<String, Object> session){
		this.session = session;
	}

}


